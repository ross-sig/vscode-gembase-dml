    "SWITCH_BASE_SPEC": {
        "prefix": "SWITCH_BASE \"form_file_spec\" [form_name] [(arguments)]",
        "body": [
            "SWITCH_BASE ${form_file_spec} ${form} (${arguments})"
        ],
        "description": "AUtomatically exit current form by calling DMC or DML and execute form"
