                "\t/SORTED_BY=${sorted_by} &",
                "\t ! or &",
                "\t/SORTED_BY=${sorted_by} &",
                "\t/STATISTIC=${variable}=${statistic_function} &",
                "\t/TABLE=${table} &",
                "\t ! or &",
                "\t/TABLE=${table} &",
                "\t/WITH=${with_field_expression}",
                "\t ! or &",
                "\t/WITH=${with_field_expression}",
