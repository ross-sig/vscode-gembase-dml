                "\t/LEN=${len_num} &",
                "\t/LOV=${lov_table}(${lov_fields}) &",
                "\t/LOV_AUTO_SELECT &",
                "\t/LOV_COL=${lov_col} &",
                "\t/LOV_DATA=${lov_data_field} &",
                "\t/LOV_FIRST=${lov_first} &",
                "\t/LOV_HEIGHT=${lov_height} &",
                "\t/LOV_NOHEADING &",
                "\t/LOV_NOSEARCH &",
                "\t/LOV_REDUCED_TO=${lov_reduced_to} &",
                "\t/LOV_ROW=${lov_row_num} &",
                "\t/LOV_SECONDARY &",
